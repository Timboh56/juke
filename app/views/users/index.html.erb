<div class="users_index">
	<% if @users %>
	  <%= render partial: "admin_panel", locals: { :users => @users } %>
	<% else %>
		<h1> Welcome. We're so glad you're here. </h1>
		<div class="user_pane">
	
		<% if current_user.oauth_token %>
		Logged into Facebook as <%= current_user.username %>.
		<% else %>
			<h1> Email: </h1> <% current_user.email %>
			<h1> Username: </h1> <% current_user.username %>
		<% end %>
		<p>
			<center>
			<%= link_to "Edit My Profile", edit_user_path(current_user.id), :class => "btn btn-default" %>
		
		  <%= link_to "Find a jukebox", "jukeboxes", :class => "btn btn-large btn-primary" %>
		</center>
	<% end %>
</div>