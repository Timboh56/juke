<%= render "layouts/error_messages", :target => @vote %>

<%= form_for(@vote) do |f| %>

  <div class="partial_field">
    <%= f.label :song_title %><br />
    <%= f.text_field :song_title %>
  </div>
  <div class="partial_field">
    <%= f.label :artist %><br />
    <%= f.text_field :artist %>
  </div>
  <div class="partial_field">
    <%= f.label :url %><br />
    <%= f.text_field :url %>
  </div>
  <br>
  <div class="actions">
    <%= f.submit "Submit your song!" %>
  </div>
<% end %>
