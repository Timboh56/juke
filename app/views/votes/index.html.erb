<h1>Listing votes</h1>

<table>
  <tr>
    <th>User</th>
    <th>Song title</th>
    <th>Artist</th>
    <th>Url</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @votes.each do |vote| %>
  <tr>
    <td><%= vote.user %></td>
    <td><%= vote.song_title %></td>
    <td><%= vote.artist %></td>
    <td><%= vote.url %></td>
    <td><%= link_to 'Show', vote %></td>
    <td><%= link_to 'Edit', edit_vote_path(vote) %></td>
    <td><%= link_to 'Destroy', vote, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Vote', new_vote_path %>
